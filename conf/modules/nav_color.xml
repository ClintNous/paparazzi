<!DOCTYPE module SYSTEM "module.dtd">

<module name="nav_color">
  <doc>
    <description>navigation functions flightplan</description>
  </doc>
  
  <settings>
    <dl_settings>
      <dl_settings name="nav_color">
        <dl_setting var="b_damp" min="0" step="0.1" max="20" shortname="b_damp" module="nav_color/nav_color"/>
	<dl_setting var="K_goal" min="0" step="0.1" max="50" shortname="K_goal" module="nav_color/nav_color"/>
	<dl_setting var="K_obst" min="0" step="0.1" max="50" shortname="K_obst" module="nav_color/nav_color"/>
	<dl_setting var="c1" min="0" step="0.1" max="10" shortname="c1" module="nav_color/nav_color"/>
	<dl_setting var="c2" min="0" step="0.1" max="10" shortname="c2" module="nav_color/nav_color"/>
	<dl_setting var="c3" min="0" step="0.1" max="10" shortname="c3" module="nav_color/nav_color"/>
	<dl_setting var="c5" min="0" step="0.1" max="10" shortname="c5" module="nav_color/nav_color"/>
	
      </dl_settings>
    </dl_settings>
  </settings>
  
  <header>
    <file name="nav_color.h" dir="modules/nav_color"/>
  </header>
  
  <init fun="nav_color_init()"/>
  <periodic fun="nav_color_period()" freq="10" autorun="LOCK"/>
    <makefile>
    <file name="nav_color.c" dir="modules/nav_color"/>
  </makefile>
  
</module>

